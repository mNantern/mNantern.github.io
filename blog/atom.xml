---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Matthieu Nantern - Blog</title>
  <link href="http://blog.nantern.com/"/>
  <link type="application/atom+xml" rel="self" href="http://blog.nantern.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.nantern.com/</id>
  <author>
    <name>Matthieu Nantern</name>
    <email>matthieu.nantern@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>{{site.url}}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://{{site.url}}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Matthieu Nantern</name>
      <uri>http://nantern.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
